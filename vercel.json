{
    "version": 2,
    "builds": [
      {
        "src": "client-main/client-main/client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client-main/client-main/client/build/$1"
      }
    ]
  }
  